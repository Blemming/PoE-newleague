{
    "name": "PoE-leveling",
    "version": "1.0.0",
    "description": "Quick and dirty leveling guide website for poe",
    "author": "Blemming",
    "private": true,
    "scripts": {
        "gen:md": "node ./tools/mdparser.js",
        "gen:gems": "node ./tools/updategemdata.js",
        "data": "node ./tools/mdparser.js && node ./tools/updategemdata.js",
        "dev": "nuxt",
        "build": "npm run data && nuxt build",
        "start": "nuxt start",
        "generate": "npm run data && nuxt generate",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
    },
    "dependencies": {
        "@nuxtjs/markdownit": "^2.0.0",
        "core-js": "^3.15.1",
        "just-clone": "^5.0.1",
        "markdown-it-attrs": "^4.1.3",
        "markdown-it-div": "^1.1.0",
        "markdown-it-task-lists": "^2.1.1",
        "node-fetch": "^2.6.7",
        "nuxt": "^2.15.7",
        "v-click-outside": "^3.1.2"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.14.7",
        "@nuxt/postcss8": "^1.1.3",
        "@nuxtjs/eslint-config": "^6.0.1",
        "@nuxtjs/eslint-module": "^3.0.2",
        "autoprefixer": "^10.4.2",
        "eslint": "^7.29.0",
        "eslint-plugin-nuxt": "^2.0.0",
        "eslint-plugin-vue": "^7.12.1",
        "postcss": "^8.4.6",
        "tailwindcss": "^3.0.18"
    }
}